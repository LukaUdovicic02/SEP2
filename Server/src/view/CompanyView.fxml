<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="174.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CompanyView">
   <children>
      <HBox layoutY="8.0" prefHeight="53.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="217.0">
               <children>
                  <Label fx:id="nameLabel" text="Name">
                     <font>
                        <Font size="33.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="381.0" spacing="30.0">
               <children>
                  <Label text="Current price:">
                     <font>
                        <Font size="33.0" />
                     </font>
                  </Label>
                  <Label fx:id="priceLabel" text="price">
                     <font>
                        <Font size="33.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="91.0" prefHeight="69.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#backOnAction" prefHeight="49.0" prefWidth="90.0" text="Back">
               <font>
                  <Font size="26.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
